//Catalan Number 
 class   Solution   { 
 public : 
      int   numTrees ( int  n )   { 
         vector < int >   dp ( n + 1 ) ; 
         dp [ 0 ] = 1 ; 
         dp [ 1 ] = 1 ; 
          for ( int  i = 2 ;  i <= n ;  i ++ )  
              for ( int  j = 0 ;  j < i ;  j ++ ) 
                 dp [ i ] += dp [ i - j - 1 ] * dp [ j ] ;   // 1 to j-1 * j+1 to i {j+1 to i can be written as 0 to i-j} 
          return  dp [ n ] ; 
      } 
 } ;